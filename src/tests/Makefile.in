# $Id$

SHELL = @SHELL@
top_srcdir = @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@
CCDEPMODE = @CCDEPMODE@

@SET_MAKE@

CXXFLAGS = @CXXFLAGS@ @CPPUNIT_CFLAGS@ -I.. -I$(top_srcdir) -I$(top_srcdir)/build -I$(top_srcdir)/lib @DEFS@ $(CFLGS) '-DREVISION="$(REVISION)"' -DBUILDTS=$(BUILDTS)

.PHONY: units clean distclean

include $(top_srcdir)/build/build.mk

OBJS =

units: $(CORE_UNITS)

$(CORE_UNITS): $(OBJS)
	@echo "[AR]	$@"
	ar rcs $@ $(OBJS)
#	@$(LD) -o $@ $(OBJS)

clean:
	@rm -f *.o $(CORE_UNITS)

distclean:
	@rm -f Makefile

include ./.deps/includes
