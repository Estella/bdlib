# $Id$

SHELL = @SHELL@
top_srcdir = @top_srcdir@
srcdir = @srcdir@
libdir = $(top_srcdir)/lib
VPATH = @srcdir@
CCDEPMODE = @CCDEPMODE@

@SET_MAKE@

CXXFLAGS = @CXXFLAGS@ -I.. -I$(top_srcdir) -I$(top_srcdir)/build -I$(libdir)/lib/wraith -I$(libdir)/lib/bdlib -I$(libdir)/openssl @DEFS@ $(CFLGS) '-DREVISION="$(REVISION)"' -DBUILDTS=$(BUILDTS)

.PHONY: units clean distclean

include $(top_srcdir)/build/build.mk

OBJS =

units: $(CORE_UNITS)

$(CORE_UNITS): $(OBJS)
	ar rcs $@ $(OBJS)
#	@$(LD) -o $@ $(OBJS)

clean:
	@rm -f *.o $(CORE_UNITS)

distclean:
	@rm -f Makefile

include ./.deps/includes
