# $Id$

SHELL = @SHELL@
top_srcdir = @top_srcdir@
srcdir = @srcdir@
libdir = $(top_srcdir)/lib
VPATH = @srcdir@
CCDEPMODE = @CCDEPMODE@

@SET_MAKE@

CXXFLAGS = @CXXFLAGS@ @CPPUNIT_CFLAGS@ -I.. -I$(top_srcdir) -I$(top_srcdir)/build -I$(libdir)/openssl @DEFS@ $(CFLGS) '-DREVISION="$(REVISION)"' -DBUILDTS=$(BUILDTS)

.PHONY: units clean distclean

include $(top_srcdir)/build/build.mk

OBJS = StringTest.o

units: $(BDLIB_UNITS)

$(BDLIB_UNITS): $(OBJS)
	ar rcs $@ $(OBJS)
#	@$(LD) -o $@ $(OBJS)

clean:
	@rm -f *.o $(BDLIB_UNITS)

distclean:
	@rm -rf Makefile .deps

include ./.deps/includes
