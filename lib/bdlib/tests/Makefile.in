SHELL = @SHELL@
top_srcdir = @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@
CCDEPMODE = @CCDEPMODE@

@SET_MAKE@

CXXFLAGS = @CXXFLAGS@ @CPPUNIT_CFLAGS@ -I.. -I$(top_srcdir) -I$(top_srcdir)/build @DEFS@ $(CFLGS) '-DREVISION="$(REVISION)"' -DBUILDTS=$(BUILDTS)

.PHONY: units clean distclean

UNITLIB = $(top_srcdir)/lib/bdlib_units.la

OBJS = StringTest.o

units: $(UNITLIB)

$(UNITLIB): $(OBJS)
	ar rcs $@ $(OBJS)
#	@$(LD) -o $@ $(OBJS)

clean:
	@rm -f *.o $(UNITLIB)

distclean:
	@rm -f Makefile

include ./.deps/includes
include $(top_srcdir)/build/build.mk
